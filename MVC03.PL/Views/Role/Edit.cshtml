@model RoleToReturnDto
@{
    ViewData["Title"] = "Edit";
}

<h1>Edit PAge</h1>
<br />
<br />

<form asp-action="Edit" enctype="multipart/form-data">

    <partial name="RolePartialView/_CreateEditPartialView" model="@Model"/>

    <a asp-action="AddOrRemoveUser" asp-route-roleId="@Model.Id" class="btn btn-success">Add or Remove User </a>


    <div class="my-3">
        <input type="submit" class="btn btn-primary" value="Save" />
        @* <a asp-action="Edit" class="btn btn-warning">Save</a> *@
        <a asp-action="Index" class="btn btn-light">Back To List </a>
    </div>
</form>


@section ValidationSection {
    <script src="~/lib/jquery-validation/dist/jquery.validate.min.js"></script>
    <script src="~/lib/jquery-validation-unobtrusive/jquery.validate.unobtrusive.min.js"></script>
}

